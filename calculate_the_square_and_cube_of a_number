#include <stdio.h>

int main() {
    float number, square, cube;

    // Input a number
    printf("Enter a number: ");
    scanf("%f", &number);

    // Calculate square and cube
    square = number * number;
    cube = number * number * number;

    // Output the results
    printf("Square of %.2f is %.2f\n", number, square);
    printf("Cube of %.2f is %.2f\n", number, cube);

    return 0;
}
